.gameDetails {
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}
.gameDetails .table {
  height: 346px;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  background-repeat: no-repeat;
}
.gameDetails .table .currentOperation {
  position: absolute;
  left: 50%;
  top: 40%;
}
.gameDetails .table .seat {
  height: 70rpx;
  position: absolute;
}
.gameDetails .table .seat .cards {
  display: flex;
  height: 50rpx;
}
.gameDetails .table .seat .cards .card {
  border: 1px solid #000;
  width: 35rpx;
  margin-right: 3rpx;
}
.gameDetails .table .seat .chips {
  font-size: 12rpx;
}
.gameDetails .table .seat0 {
  left: 28%;
  top: 20rpx;
}
.gameDetails .table .seat1 {
  right: 37%;
  top: 20rpx;
}
.gameDetails .table .seat2 {
  right: 13%;
  top: 50rpx;
}
.gameDetails .table .seat3 {
  right: 13%;
  bottom: 20rpx;
}
.gameDetails .table .seat4 {
  right: 30%;
  bottom: 6rpx;
}
.gameDetails .table .seat5 {
  left: 43%;
  bottom: 6rpx;
}
.gameDetails .table .seat6 {
  left: 25%;
  bottom: 6rpx;
}
.gameDetails .table .seat7 {
  left: 8%;
  bottom: 20rpx;
}
.gameDetails .table .seat8 {
  left: 8%;
  top: 50rpx;
}
.gameDetails .operationBar {
  position: absolute;
  right: 10rpx;
  top: 43%;
}
.gameDetails .operationBar .btnList {
  display: flex;
  flex-direction: column;
}
.gameDetails .operationBar .btnList button {
  font-size: 12px;
}

